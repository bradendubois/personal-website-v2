{
  "compilerOptions": {
    "module": "esnext",
    "sourceMap": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "target": "es6",
    "jsx": "react",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src",
    "../../../.WebStorm2019.2/config/javascript/nodejs/12.13.0/node-typings/node_modules/@types/node/globals.d.ts"
  ]
}
